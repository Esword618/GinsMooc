    "description": "A Chrome extension to get the mooc answers and evaluate auto automatically.",
    "icons": {
        "16": "icons/favicon16.png",
        "32": "icons/favicon32.png",
        "48": "icons/favicon48.png",
        "128": "icons/favicon128.png"
    },
    "author": { "email": "ginnnnnn-cc@outlook.com" },
    "homepage_url": "https://ginnnnnn.top/mooc",
    "content_scripts": [
        {
            "matches": ["https://www.icourse163.org/*learn/*?tid=*"],
            "js": ["content-scripts/index-d96f628c.js"],
            "run_at": "document_end"
        }
    ]
}